{
  "pluginName": "百度翻译",
  "description": "使用百度翻译 api 实现的一款翻译插件",
  "author": "Eliot.Ye",
  "main": "index.html",
  "logo": "logo.png",
  "version": "0.1.2",
  "features": [
    {
      "code": "百度翻译",
      "explain": "百度翻译",
      "cmds": [
        "百度翻译",
        "翻译",
        {
          "type": "regex",
          "label": "翻译",
          "match": "^[A-Za-z]+$"
        }
      ]
    }
  ]
}
